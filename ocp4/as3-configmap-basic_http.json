{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema.json",
    "class": "AS3",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.23.0",
        "id": "f5demo",
        "label": "CIS AS3 Example",
        "remark": "Example of using CIS ConfigMap",
        "ConfigMap": {
            "class": "Tenant",
            "MyApps": {
                "class": "Application",
                "template": "generic",
                "frontend": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.10.101"
                    ],
                    "remark": "frontend: f5demo.tcp.v1",
                    "persistenceMethods": [],
                    "virtualPort": 80,
                    "pool": "frontend2_pool",
                    "profileHTTPCompression": { "use": "compression_front" }
                },
                "frontend2_pool": {
                    "class": "Pool",
                    "monitors": [
                        "tcp"
                    ],
                    "members": [
                        {
                            "servicePort": 8080,
                            "serverAddresses": [],
                            "shareNodes": true
                        }
                    ]
                },
                "compression_front": {
                    "class": "HTTP_Compress",
                    "minimumSize": 2048,
                    "cpuSaver": true,
                    "cpuSaverLow": 60,
                    "cpuSaverHigh": 70
                }
            }
        }
    }
}